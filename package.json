	"devDependencies": {
		"@jonahsnider/prettier-config": "1.1.2",
		"@jonahsnider/xo-config": "5.3.0",
		"@tsconfig/node18-strictest-esm": "1.0.1",
		"@types/node": "18.11.18",
		"@vitest/coverage-c8": "0.25.8",
		"@vitest/ui": "0.25.8",
		"npm-run-all": "4.1.5",
		"prettier": "2.8.3",
		"type-fest": "3.5.4",
		"typescript": "4.9.5",
