{
	"name": "@jonahsnider/aoc-2022",
	"version": "1.0.0",
	"private": true,
	"description": "My Advent of Code 2022 solutions in TypeScript.",
	"keywords": [
		"typescript",
		"starter"
	],
	"bugs": {
		"url": "https://github.com/jonahsnider/aoc-2022/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/jonahsnider/aoc-2022.git"
	},
	"license": "Apache-2.0",
	"author": {
		"name": "Jonah Snider",
		"email": "jonah@jonahsnider.com",
		"url": "https://jonahsnider.com"
	},
	"type": "module",
	"exports": "./tsc_output/index.js",
	"main": "./tsc_output/index.js",
	"scripts": {
		"build": "tsc",
		"lint": "xo",
		"start": "node .",
		"style": "prettier --check .",
		"test": "ava",
		"test:coverage": "c8 --reporter lcov --reporter text-summary --include 'src/**/*.ts' --exclude 'src/types/**/*.ts' --exclude 'src/**/*.test.ts' --all ava"
	},
	"prettier": "@jonahsnider/prettier-config",
	"dependencies": {
		"@jonahsnider/benchmark": "5.0.0",
		"@jonahsnider/util": "9.0.1",
		"clipanion": "3.2.0-rc.14",
		"convert": "4.7.1"
	},
	"devDependencies": {
		"@jonahsnider/prettier-config": "1.1.2",
		"@jonahsnider/xo-config": "5.2.0",
		"@tsconfig/node18-strictest-esm": "1.0.1",
		"@types/node": "18.11.10",
		"ava": "5.1.0",
		"c8": "7.12.0",
		"prettier": "2.8.0",
		"ts-node": "10.9.1",
		"type-fest": "3.3.0",
		"typescript": "4.9.3",
		"xo": "0.53.1"
	},
	"packageManager": "yarn@3.3.0"
}
